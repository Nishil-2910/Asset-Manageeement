    //Bind Employee Details
    function BindEmployee(Search) {
        $("#drp_allocate").empty();
        var url = '@Url.Content("~/Asset/GetAllocationEmployee")';
        $.ajax({
            type: "POST",
            async: false,
            url: url,
            data: { Search: Search },
            datatype: "Json",
            success: function (result) {
                /*$("#drp_allocate").append('<option value="">Select Employee</option>'),
                    $.each(result, function (result, a) { $("#drp_allocate").append('<option value="' + a.Drpvalu + '">' + a.Drptext + "</option>") });
                $("#drp_allocate").change();*/


                var DrpObj = document.getElementById("drp_allocate");
                for (var i = 0; i < DrpObj.length; i = i) {
                    DrpObj.remove(i);
                }
                //var opt = document.createElement("option");
                //DrpObj.options.add(opt);

                var opt1 = document.createElement("option");
                DrpObj.options.add(opt1);
                opt1.text = "---Select---";
                opt1.value = "0";

                $.each(result, function (result, a) {
                    var opt = document.createElement("option");
                    DrpObj.options.add(opt);
                    opt.text = a.Drptext;
                    opt.value = a.Drpvalu;
                });
                $("#drp_allocate").val("0").change();
            }
        });
    }
    function BindEmployeeDetails(Empno) {
        $.ajax({
            url: "../Asset/GetAllocationEmployeeDetails?Empno=" + Empno,
            type: 'GET',
            async: false,
            contentType: 'application/json; charset=utf-8',
            dataType: 'json',
            success: function (a) {
                $("#txt_department").val(a.DEPTNAME);
                $("#txt_designation").val(a.DESIGNATION);
                $("#txt_location").val(a.Locationname);
            }, error: function (a) { alert(a) }
        });
    }

    //Bind Hostname with SerialNo
    function BindHostname(Vserialno) {
        $("#txt_hostname").val('');
        $.ajax({
            url: "../Asset/GetHostnameDetails?Vserialno=" + Vserialno,
            type: 'GET',
            async: false,
            contentType: 'application/json; charset=utf-8',
            dataType: 'json',
            success: function (a) {
                if (a.Hostname == null) {
                    $("#txt_hostname").prop("readonly", false);
                }
                else {
                    $("#txt_hostname").prop("readonly", true);
                    $("#txt_hostname").val(a.Hostname);
                }
            }, error: function (a) { alert(a) }
        });
    }

    $('#drp_serialno').change(function () {
        BindHostname($('#drp_serialno').val());
    });

    //Bind Laptop / Desktop
    function BindItem() {
        $("#drp_item").empty();
        var url = '@Url.Content("~/Asset/GetItemType")';
        $.ajax({
            type: "POST",
            url: url,
            datatype: "Json",
            success: function (result) {
                $("#drp_item").append('<option value="">Select Item</option>'),
                    $.each(result, function (result, a) {
                        var str = a.Drptext;
                        var n = str.substring(0, 7);
                        if (n != 'Monitor' && n!='Printer') {
                            $("#drp_item").append('<option value="' + a.Drpvalu + '">' + a.Drptext + "</option>")
                        }
                    });
            }
        });
    }
