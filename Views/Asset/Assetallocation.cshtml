@{
    ViewBag.Title = "Assetallocation";
    var regno = ViewBag.Reqno;
    var empno = ViewBag.Empno;
    string cLayout = "";
    if ((regno != null && empno != null))
    {
        cLayout = "~/Views/Shared/_Layoutblk.cshtml";
    }
    else
    {
        cLayout = "~/Views/Shared/_Layout.cshtml";
    }
    Layout = cLayout;
}
<script>
    function SubmitAllocationRequest() {
        var formid = 'frm_assetAllocation';
        var obj = validateForm(formid);
        if (obj.form()) {
            var Form = document.getElementById(formid);
            Form.action = '@Url.Action("SubmitAllocationRequest", "Asset")';
            $("#" + formid).ajaxForm({
                iframe: true,
                dataType: "json",
                success: function (result) {
                    if (result.MessageCode == "-1") {
                        ShowError(result.message);
                        return false;
                    }
                    else {
                        ShowSuccess(result.message);
                        $("#" + formid).action = "";
                        $("#" + formid).trigger("reset");
                        $("#refresh_jqTable2").click();
                    }
                },
                error: function (xhr, textStatus, errorThrown) {
                    alert("error");
                    ShowError("Error Occured Submiting Asset DeAllocation Request. Try Restarting your current session.");
                }
            });
        }
        else {
            var Form = document.getElementById(formid);
            Form.action = "";
            return false;
        }
    }
    function validateForm(formid) {
        return $("#" + formid).validate({
            errorElement: 'div',
            errorClass: 'help-block',
            focusInvalid: false,
            ignore: "",
            rules: {
                drp_allocate: {
                    required: true
                },
                txt_allocationdate: {
                    required: true
                },
                drp_compliance: {
                    required: true
                },
                drp_Classification: {
                    required: true
                },
                drp_domain: {
                    required: true
                },
                drp_OS: {
                    required: true
                },
                txt_remarks: {
                    required: true
                }
            },
            messages: {
                drp_allocate: {
                    required: "Please Select Employee."
                },
                txt_allocationdate: {
                    required: "Please Enter Allocation Date."
                },
                drp_compliance: {
                    required: "Please select GXP / NONGXP"
                },
                drp_Classification: {
                    required: "Please select Classfication"
                },
                drp_domain: {
                    required: "Please select Domain"
                },
                drp_OS: {
                    required: "Please Select Operating System"
                },
                txt_remarks: {
                    required: "Please Enter remarks"
                }
            },

            highlight: function (e) {
                $(e).closest('.form-group').removeClass('has-info').addClass('has-error');
            },

            success: function (e) {
                $(e).closest('.form-group').removeClass('has-error'); //.addClass('has-info');
                $(e).remove();
            },

            errorPlacement: function (error, element) {
                if (element.is('input[type=checkbox]') || element.is('input[type=radio]')) {
                    var controls = element.closest('div[class*="col-"]');
                    if (controls.find(':checkbox,:radio').length > 1) controls.append(error);
                    else error.insertAfter(element.nextAll('.lbl:eq(0)').eq(0));
                }
                else if (element.is('.select2')) {
                    error.insertAfter(element.siblings('[class*="select2-container"]:eq(0)'));
                }
                else if (element.is('.chosen-select')) {
                    error.insertAfter(element.siblings('[class*="chosen-container"]:eq(0)'));
                }
                else error.insertAfter(element.parent());
            },

            submitHandler: function (form) {
            },
            invalidHandler: function (form) {
            }
        });
    }
</script>
