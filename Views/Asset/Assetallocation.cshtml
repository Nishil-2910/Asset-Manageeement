    function BindSerialNo(ItemNo, Receipt) {
        $("#drp_serialno").empty();
        var url = '@Url.Content("~/Asset/GetSerialNo")';
        $.ajax({
            type: "POST",
            url: url,
            data: { ItemNo: ItemNo, Receipt: Receipt },
            datatype: "Json",
            success: function (result) {
                $("#drp_serialno").append('<option value="">Select Serial No</option>'),
                    $.each(result, function (result, a) { $("#drp_serialno").append('<option value="' + a.Drpvalu + '">' + a.Drptext + "</option>") });
                //$("#drp_serialno").select2();
            }
        });
    }
