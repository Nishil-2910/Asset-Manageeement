    $(document).ready(function () {
        var Empno = $('#hdn_Empno').val();
        $('#div_monitor').hide();
        $('#div_printer').hide();

        if (Empno != "") {
            $('#txt_search').val(Empno);
            BindEmployee(Empno);
            $('#drp_allocate').val(Empno);
            BindEmployeeDetails(Empno);
        } else {

        }

        $('#btn_search').click(function () {
            if ($('#txt_search').val().length > 0) {
                BindEmployee($('#txt_search').val());
                //$('#drp_allocate').val($('#txt_search').val()).change();
                //BindEmployeeDetails($('#drp_allocate').val());
                //$('#drp_allocate').val($('#txt_search').val()).select;

            }
            else {
                alert("Please Enter Employee No. / Name.");
            }
        });

        $('#drp_allocate').change(function () {
            if ($('#drp_allocate').val() != "0")
                BindEmployeeDetails($('#drp_allocate').val());
            else {
                $("#txt_department").val("");
                $("#txt_designation").val("");
                $("#txt_location").val("");
            }

        });

        $('#drp_receipt').change(function () {
            BindSerialNo($('#drp_item').val(), $('#drp_receipt').val());
        });

        $('#drp_item').change(function () {
            BindSerialNo($('#drp_item').val(), $('#drp_receipt').val());
        });

        BindItem();
        BindMonitor();
        BindPrinter();
        GetGXP();
        GetClassfication();
        BindDomain();
        BindOS();

        $('#drp_item_monitor').change(function () {
            BindMonitorSerialNo($('#drp_item_monitor').val(), $('#drp_receipt').val());
        });

        $('#drp_item_printer').change(function () {
            BindPrinterSerialNo($('#drp_item_printer').val(), $('#drp_receipt').val());
        });

        //var select2dropdwon = ["#drp_allocate", "#drp_item", "#drp_serialno"];
        //$.each(select2dropdwon, function (index, value) {
        //    $(value).select2();
        //});

        var datecontrols = ["#txt_allocationdate"];

        $.each(datecontrols, function (index, value) {
            $(value).datepicker('setDaysOfWeekDisabled', [0]);
        });
    });
    function EnabledMonitor() {
        if ($('#chk_monitor').is(":checked")) {
            $('#div_monitor').show();
        } else {
            $('#div_monitor').hide();
        }
    }

    function EnabledPrinter() {
        if ($('#chk_printer').is(":checked")) {
            $('#div_printer').show();
        } else {
            $('#div_printer').hide();
        }
    }
